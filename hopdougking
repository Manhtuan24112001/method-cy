local Players = game:GetService("Players")
local level = Players.LocalPlayer.Data.Level.Value
if level <= 2200 then
spawn(function()
    while wait(30) do
        pcall(function()
            if game.Workspace.Enemies:FindFirstChild('Dough King') or game.ReplicatedStorage:FindFirstChild('rip_indra True Form') then
                print("Boss rip_indra True Form đã xuất hiện!")
            else
                print("Boss rip_indra True Form chưa xuất hiện. Đang tải script hopbossbebe...")
                loadstring(game:HttpGet("https://raw.githubusercontent.com/Manhtuan24112001/method-cy/refs/heads/main/hopbossbossbebe2"))()
            end
        end)
    end
end)
