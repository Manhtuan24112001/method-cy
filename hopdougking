spawn(function()
    while wait(30) do
        pcall(function()
            if game.Workspace.Enemies:FindFirstChild('Dough King') or game.ReplicatedStorage:FindFirstChild('Dough King') then
                print("Boss Dough King đã xuất hiện!")
            else
                print("Boss Dough King chưa xuất hiện. Đang tải script hopbossbebe...")
                loadstring(game:HttpGet("https://raw.githubusercontent.com/Manhtuan24112001/method-cy/refs/heads/main/hopbossbossbebe2"))()
            end
        end)
    end
end)
